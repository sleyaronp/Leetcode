public class randomtest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println('A'); //A
		System.out.println( 1 + 'A'); //ASCII of character next to A
		System.out.println((char)(1 + 'A')); //actual character next to A
		System.out.println((int)('A')); //ASCII number of character A
	}

}
